page {
  padding: 5px 10px;
  box-sizing: border-box;
  min-height: calc(100vh - 60px);
  background-color: rgba(220, 220, 220, 0.3);
}
.primary-btn {
  background: #8B0206;
  width: 150px;
  border-radius: 40px;
  color: #eeeeee;
}
.row-flex {
  display: flex;
  justify-content: center;
  align-items: center;
}
